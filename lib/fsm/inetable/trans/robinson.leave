#!/bin/sh -e
. ../common.sh
. ../common-queen.sh
logmessage "Running script: $0"

#Remove IPv4 Addr
mesh_del_ipv4

# stop DHCP server 
mesh_remove_dhcp
/etc/init.d/dnsmasq restart


# stop redirecting all inet traffic to local httpd/nameserver
iptables -t nat -F robinson_inet_$interface

# TODO: now that we have internet, send all packets destined to the
# fake net to the intended inet host; for now reject them
iptables -t nat -F robinson_fake_$interface
